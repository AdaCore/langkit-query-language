with "liblkqllang";
with "libadalang";

project Lkql_Interpreter is
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("main.adb");
   for Ignore_Source_Sub_Dirs use ("lkql");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnat12", "-g", "-O0", "-gnata",
                                        "-gnatwa", "-gnatwe", "-gnatyg");
   end Compiler;
end Lkql_Interpreter;
