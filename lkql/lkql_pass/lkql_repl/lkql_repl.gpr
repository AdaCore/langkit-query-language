with "lkql_interpreter";
with "${lib_name.lower}";

project Lkql_Repl is
    for Source_Dirs use ("src");
    for Object_Dir use "obj";
    for Main use ("repl.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnat12", "-g", "-O0", "-gnata",
                                        "-gnatwa");
   end Compiler;
end Lkql_Repl;
